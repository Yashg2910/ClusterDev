<body>
<table class="table table-striped table-primary">
  <thead>
    <tr>
      <th>package_name</th>
      <th>unique_devices</th>
    </tr>
  </thead>
  <tbody>
    <%if (devices.length >0){ %>
    <% devices.forEach(function(device){ %>
    <tr>
      <td><%= device._id %></td>
      <td><%= device.unique_devices %></td>
    </tr>
    <% })}%>
  </tbody>
</table>
</body>